{
  "expo": {
    "name": "wefitness-trainer",
    "slug": "wefitness-trainer",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.zotovda.wefitnesstrainers",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Приложение использует камеру, чтобы обновить фотографию профиля.",
        "NSPhotoLibraryUsageDescription": "Приложение использует фото, чтобы обновить фотографию профиля.",
        "NSPhotoLibraryAddUsageDescription": "Приложение использует фото, чтобы обновить фотографию профиля."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
